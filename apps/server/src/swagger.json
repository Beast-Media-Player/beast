  "components": {
    "schemas": {
      "LoginBody": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "nullable": false,
        "required": [
          "email",
          "password"
        ]
      },
      "AuthTokens": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string"
          },
          "refresh_token": {
            "type": "string"
          }
        },
        "nullable": false,
        "required": [
          "access_token",
          "refresh_token"
        ]
      },
      "RefreshBody": {
        "type": "object",
        "properties": {
          "refresh_token": {
            "type": "string"
          }
        },
        "nullable": false,
        "required": [
          "refresh_token"
        ]
      },
      "RegisterBody": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "maxLength": 64
          },
          "password": {
            "type": "string",
            "maxLength": 64,
            "minLength": 8
          }
        },
        "nullable": false,
        "required": [
          "email",
          "password"
        ]
      },
      "LibraryType": {
        "type": "string",
        "enum": [
          "MOVIES",
          "TV_SHOWS"
        ]
      },
      "CreateLibraryDTO": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "MOVIES",
              "TV_SHOWS"
            ]
          },
          "name": {
            "type": "string"
          },
          "path": {
            "type": "string"
          }
        },
        "nullable": false,
        "required": [
          "type",
          "name",
          "path"
        ]
      },
      "QueryLibrary": {
        "type": "object",
        "properties": {
          "libraryId": {
            "type": "string"
          }
        },
        "nullable": false,
        "required": [
          "libraryId"
        ]
      },
      "EditLibraryPermissions": {
        "type": "object",
        "properties": {
          "libraryId": {
            "type": "string"
          },
          "add": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "userId": {
                  "type": "string"
                },
                "access": {
                  "type": "string",
                  "enum": [
                    "READ",
                    "WRITE"
                  ]
                }
              },
              "nullable": false,
              "required": [
                "userId",
                "access"
              ]
            }
          },
          "remove": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "userId": {
                  "type": "string"
                },
                "access": {
                  "type": "string",
                  "enum": [
                    "READ",
                    "WRITE"
                  ]
                }
              },
              "nullable": false,
              "required": [
                "userId",
                "access"
              ]
            }
          }
        },
        "nullable": false,
        "required": [
          "libraryId"
        ]
      },
      "LibraryDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "$ref": "#/components/schemas/LibraryType"
          },
          "name": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "nullable": false,
        "required": [
          "id",
          "type",
          "name",
          "path",
          "createdAt"
        ]
      },
      "LibraryContent": {
        "oneOf": [
          {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/LibraryContentShow"
            }
          },
          {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/LibraryContentMovie"
            }
          }
        ]
      },
      "LibraryContentShow": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "TV_SHOWS"
            ]
          },
          "data": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "tvmazeId": {
                "type": "number"
              },
              "name": {
                "type": "string"
              },
              "path": {
                "type": "string"
              },
              "overview": {
                "type": "string",
                "nullable": true
              },
              "images": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "libraryId": {
                "type": "string"
              }
            },
            "nullable": false,
            "required": [
              "id",
              "tvmazeId",
              "name",
              "path",
              "overview",
              "images",
              "libraryId"
            ]
          }
        },
        "nullable": false,
        "required": [
          "type",
          "data"
        ]
      },
      "LibraryContentMovie": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "MOVIES"
            ]
          },
          "data": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "overview": {
                "type": "string",
                "nullable": true
              },
              "images": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "tmdbId": {
                "type": "number"
              },
              "mediaId": {
                "type": "string"
              },
              "libraryId": {
                "type": "string"
              }
            },
            "nullable": false,
            "required": [
              "id",
              "name",
              "overview",
              "images",
              "tmdbId",
              "mediaId",
              "libraryId"
            ]
          }
        },
        "nullable": false,
        "required": [
          "type",
          "data"
        ]
      },
      "QueryFilesystem": {
        "type": "object",
        "properties": {
          "root": {
            "type": "string"
          }
        },
        "nullable": false,
        "required": [
          "root"
        ]
      },
      "QueryMedia": {
        "type": "object",
        "properties": {
          "mediaId": {
            "type": "string"
          }
        },
        "nullable": false,
        "required": [
          "mediaId"
        ]
      },
