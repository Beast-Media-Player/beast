    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nestia": "^5.1.2",
    "openapi-typescript-codegen": "^0.27.0",
    "prettier": "^3.0.0",
    "prisma": "^5.8.0",
