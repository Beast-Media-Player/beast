{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "dotEnv": [".env.production.local", ".env.local", ".env.production", ".env"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "swagger": {},
    "prisma:gen": {},
    "prisma:push": {},
    "dev": {
      "dotEnv": [".env.production.local", ".env.local", ".env.production", ".env"],
      "cache": false,
      "persistent": true
    }
  }
}
